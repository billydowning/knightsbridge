{
  "version": 2,
  "env": {
    "VITE_API_URL": "https://knightsbridge-app-35xls.ondigitalocean.app",
    "VITE_WS_URL": "wss://knightsbridge-app-35xls.ondigitalocean.app",
    "VITE_BACKEND_URL": "https://knightsbridge-app-35xls.ondigitalocean.app"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://knightsbridge-app-35xls.ondigitalocean.app",
      "VITE_WS_URL": "wss://knightsbridge-app-35xls.ondigitalocean.app",
      "VITE_BACKEND_URL": "https://knightsbridge-app-35xls.ondigitalocean.app"
    }
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/index.html"
    }
  ]
